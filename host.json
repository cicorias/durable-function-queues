{
  "version": "2.0",
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request"
      },
      "enableLiveMetricsFilters": true
    }
  },
  "extensions": {
    "durableTask": {
      "hubName": "%APP_TASK_HUB_NAME%",
      "storageProvider": {
        "type": "AzureStorage"
      }
    }
  }
}